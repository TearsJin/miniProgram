.water-Content {
  height: auto;
  display: flex;
  justify-content: center;
}

.water-Body {
  width: 90%;
  display: flex;
  margin-left: 10%;
  flex-direction: column;
  /* background-color: rgba(18,150,219,0.5); */
}

.water-List {
  width: 80%;
  margin-left: 10%;
  display: flex;
  align-items: center;
  border-radius: 5px 5px 5px 5px;
  height: 4rem;
  font-size: small;
  color: white;
  border-bottom: 0.8rem solid black;
  border-color: white;
  margin-top: 0.2rem;
  margin-bottom: 0.2rem;
}

.water-List-Detail {
  width: 100%;
  height: 2rem;
  border-radius: 10px;
  display: flex;
  align-items: center;
}
.water-List-Detail text{
  font-weight: 400;
  margin-top: 2px;
}

.water-Balance{
  font-size: 1rem;
  background-color: rgb(247, 197, 122);
}

