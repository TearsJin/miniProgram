.water-Content {
  height: auto;
  display: flex;
  justify-content: center;
}

.water-Body {
  width: 80%;
}

.water-List {
  width: 48%;
  float: left;
  padding: 0.5%;
  border-radius: 25px;
  font-size: small;
}

.water-List-Detail {
  width: 100%;
  padding-bottom: 100%;
  height: 0;
  border-radius: 10px;
  display: flex;
  flex-direction: column;
  align-items: center ;
}
.water-List-Detail text{
  font-weight: 400;
  margin-top: 2px;
}

.water-Balance{
  background-color: rgb(212,157,74);
  background-image: url('data:image/png;base64,');
  background-size: cover;
}

.water-Cold{
  background-color: rgb(18,150,219);
  background-image: url('data:image/png;base64,');
}

.water-Hot{
  background-color: rgb(244,63,94);
  background-image: url('data:image/png;base64,');
}

.water-Electricity{
  background-color: rgb(16,185,129);
  background-image: url('data:image/png;base64,');
}